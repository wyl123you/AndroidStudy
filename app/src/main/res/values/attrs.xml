<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GradientTextView">
        <attr name="startColor" format="reference|color" />
        <attr name="endColor" format="reference|color" />
        <attr name="shadowEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StrokeGradientTextView">
        <attr name="strokeColor" format="reference|color" />
        <attr name="strokeWidth" format="reference|dimension" />
        <attr name="strokeEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="half_BlockWidth" format="dimension" />
        <attr name="blockInterval" format="dimension" />
        <attr name="initPosition" format="integer" />
        <attr name="isClock_Wise" format="boolean" />
        <attr name="lineNumber" format="integer" />
        <attr name="moveSpeed" format="integer" />
        <attr name="blockColor" format="color" />
        <attr name="moveBlock_Angle" format="float" />
        <attr name="fixBlock_Angle" format="float" />
        <attr name="move_Interpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="ImageAuthenticationView">
        <!--滑块的高度-->
        <attr name="unitHeight" format="dimension" />
        <!--滑块的宽度-->
        <attr name="unitWidth" format="dimension" />
        <!--滑块占图片高度的比例-->
        <attr name="unitHeightScale" format="integer" />
        <!--滑块占图片宽度的比例-->
        <attr name="unitWidthScale" format="integer" />
        <!--滑块边框的图片资源-->
        <attr name="unitShadeSrc" format="reference" />
        <!--阴影部分的图片资源-->
        <attr name="unitShowSrc" format="reference" />
        <!--是否需要旋转-->
        <attr name="needRotate" format="boolean" />
        <!--验证时的误差值-->
        <attr name="deviate" format="integer" />
    </declare-styleable>

    <declare-styleable name="ImageAuthenticationView1">
        <!--滑块的高度-->
        <attr name="unitHeight1" format="dimension" />
        <!--滑块的宽度-->
        <attr name="unitWidth1" format="dimension" />
        <!--滑块占图片高度的比例-->
        <attr name="unitHeightScale1" format="integer" />
        <!--滑块占图片宽度的比例-->
        <attr name="unitWidthScale1" format="integer" />
        <!--滑块边框的图片资源-->
        <attr name="unitShadeSrc1" format="reference" />
        <!--阴影部分的图片资源-->
        <attr name="unitShowSrc1" format="reference" />
        <!--是否需要旋转-->
        <attr name="needRotate1" format="boolean" />
        <!--验证时的误差值-->
        <attr name="deviate1" format="integer" />
    </declare-styleable>


</resources>