<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StrokeGradientText">
        <attr name="startColor" format="reference|color" />
        <attr name="endColor" format="reference|color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="strokeEnable" format="boolean" />
        <attr name="strokeColor" format="color" />
        <attr name="shadowEnable" format="boolean" />
        <attr name="shadowType" format="enum">
            <enum name="linear" value="0" />
            <enum name="sweep" value="1" />
            <enum name="radial" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="GeometryView">
        <attr name="lineColor" format="color" />
        <attr name="lineWidth" format="dimension" />
        <attr name="radius1" format="dimension" />
        <attr name="recWidth" format="dimension" />
        <attr name="recHeight" format="dimension" />
        <attr name="triSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="half_BlockWidth" format="dimension" />
        <attr name="blockInterval" format="dimension" />
        <attr name="initPosition" format="integer" />
        <attr name="isClock_Wise" format="boolean" />
        <attr name="lineNumber" format="integer" />
        <attr name="moveSpeed" format="integer" />
        <attr name="blockColor" format="color" />
        <attr name="moveBlock_Angle" format="float" />
        <attr name="fixBlock_Angle" format="float" />
        <attr name="move_Interpolator" format="reference" />
    </declare-styleable>

    <declare-styleable name="ImageAuthenticationView">
        <!--滑块的高度-->
        <attr name="unitHeight" format="dimension" />
        <!--滑块的宽度-->
        <attr name="unitWidth" format="dimension" />
        <!--滑块占图片高度的比例-->
        <attr name="unitHeightScale" format="integer" />
        <!--滑块占图片宽度的比例-->
        <attr name="unitWidthScale" format="integer" />
        <!--滑块边框的图片资源-->
        <attr name="unitShadeSrc" format="reference" />
        <!--阴影部分的图片资源-->
        <attr name="unitShowSrc" format="reference" />
        <!--是否需要旋转-->
        <attr name="needRotate" format="boolean" />
        <!--验证时的误差值-->
        <attr name="deviate" format="integer" />
    </declare-styleable>

    <declare-styleable name="ImageAuthenticationView1">
        <!--滑块的高度-->
        <attr name="unitHeight1" format="dimension" />
        <!--滑块的宽度-->
        <attr name="unitWidth1" format="dimension" />
        <!--滑块占图片高度的比例-->
        <attr name="unitHeightScale1" format="integer" />
        <!--滑块占图片宽度的比例-->
        <attr name="unitWidthScale1" format="integer" />
        <!--滑块边框的图片资源-->
        <attr name="unitShadeSrc1" format="reference" />
        <!--阴影部分的图片资源-->
        <attr name="unitShowSrc1" format="reference" />
        <!--是否需要旋转-->
        <attr name="needRotate1" format="boolean" />
        <!--验证时的误差值-->
        <attr name="deviate1" format="integer" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="circle" format="boolean" />
        <attr name="radius" format="dimension" />
        <attr name="topLeftRadius" format="dimension" />
        <attr name="topRightRadius" format="dimension" />
        <attr name="bottomLeftRadius" format="dimension" />
        <attr name="bottomRightRadius" format="dimension" />
        <attr name="borderEnable" format="boolean" />
        <attr name="borderColor" format="color|reference" />
        <attr name="borderWidth" format="dimension" />
        <attr name="borderDash" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SweepImageView">
        <attr name="periodInMillis" format="integer" />
        <attr name="areaAngle" format="integer" />
        <attr name="repeat" format="boolean" />
        <attr name="startAngle" format="enum">
            <enum name="zero" value="0" />
            <enum name="quarter" value="90" />
            <enum name="half" value="180" />
            <enum name="quarters" value="270" />
        </attr>
        <attr name="sweep" format="enum">
            <enum name="radial" value="0" />
            <enum name="circle" value="1" />
            <enum name="square" value="2" />

        </attr>
        <attr name="direction" format="enum">
            <enum name="CW" value="0" />
            <enum name="CCW" value="1" />
        </attr>
        <attr name="type" format="enum">
            <enum name="show" value="0" />
            <enum name="hide" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetPadding" format="dimension" />
        <attr name="appWidgetInnerRadius" format="dimension" />
        <attr name="appWidgetRadius" format="dimension" />
    </declare-styleable>
</resources>